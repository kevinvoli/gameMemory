    <div class="wrap-input100 " >
            <label for="nom" class="input100"> description</label> 
                <input class="input100" name="nom" id="" cols="30" rows="10">
            </div> 
        <div class="wrap-input100 validate-input" >

            <label for="nbcaro">nombre de careau</label></p>
            <select name="nbcaro" class="input100" >
                <option value="4">4</option>
