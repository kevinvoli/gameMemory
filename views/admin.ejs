        <div class="wrap-input100 " >

            <label for="niveau"class="input100" >niveau pour etre admin</label></p>
           <input type="number" name="niveau" id="niveau" min="4" max="22" class="input100"  >
        </div> 
       
        <div class="wrap-input100 " >
           <label for="description" class="input100"> description</label> 
            <p><textarea class="input100" name="description" id="" cols="30" rows="10"></textarea> </p>
        </div> 
        <div class="wrap-input100 " >
            <label class="input100" for="dure">la dure du jeux</label> 
             <input type="time" name="dure" id="" class="champ"  >
        </div>

        <div class="wrap-input100 " >
            <label class="input100" for="debut">date de debut</label> 
         <input class="input100"type="datetime-local" name="debut" id="" class="champ" >
        </div>  
         
